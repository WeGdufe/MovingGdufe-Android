                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/menu_urgencyPhone"
                            style="@style/menu_item_icon"
                            android:src="@drawable/icon_phone_selector" />

                        <TextView
                            style="@style/menu_item_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/menu_busandtelphone" />

                    </LinearLayout>

                    <LinearLayout
                        style="@style/menu_item"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/menu_map"
                            style="@style/menu_item_icon"
                            android:src="@drawable/icon_life_selector" />

                        <TextView
                            style="@style/menu_item_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/menu_lifeinformation" />

                    </LinearLayout>


                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp">

                    <LinearLayout
                        style="@style/menu_item"
                        android:layout_width="wrap_content"
