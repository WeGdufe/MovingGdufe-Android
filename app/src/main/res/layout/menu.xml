            <!--图书馆-->
            <TextView
                style="@style/menu_title_label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/menu_lib" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="10dp"
                android:stretchColumns="*">

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent">

                    <LinearLayout
                        style="@style/menu_item"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/menu_searchBook"
                            style="@style/menu_item_icon"
                            android:src="@drawable/icon_search_book_selector" />

                        <TextView
                            style="@style/menu_item_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/menu_searchBook" />

                    </LinearLayout>

                    <LinearLayout
                        style="@style/menu_item"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/menu_currentBook"
                            style="@style/menu_item_icon"
                            android:src="@drawable/icon_now_borrow_selector" />

                        <TextView
                            style="@style/menu_item_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/menu_nowBorrowedBook" />

                    </LinearLayout>

                    <LinearLayout
                        style="@style/menu_item"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/menu_borrowedBook"
                            style="@style/menu_item_icon"
                            android:src="@drawable/icon_borrow_history_selector" />

                        <TextView
                            style="@style/menu_item_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/menu_pastBorrowedBook" />

                    </LinearLayout>

                    <LinearLayout
                        style="@style/menu_item"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageButton style="@style/menu_item_icon" />

                        <TextView
                            style="@style/menu_item_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />

                    </LinearLayout>
                </TableRow>


            </TableLayout>


            <!--应用汇-->
            <TextView
                style="@style/menu_title_label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/menu_other" />

            <TableLayout
                android:layout_width="match_parent"
