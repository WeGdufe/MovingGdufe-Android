<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_normal"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <!--教务系统-->
        <!--<include layout="@layout/menu_jiaowu" />-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <TextView
                style="@style/menu_title_label"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/menu_edusys" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="10dp"
                android:stretchColumns="*">

                <TableRow
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent">

                    <LinearLayout
                        style="@style/menu_item"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/menu_score"
                            style="@style/menu_item_icon"
                            android:src="@drawable/icon_score_selector" />

                        <TextView
                            style="@style/menu_item_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/menu_goal" />

                    </LinearLayout>
                    <LinearLayout
                        style="@style/menu_item"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/menu_cet"
                            style="@style/menu_item_icon"
                            android:src="@drawable/icon_cet_selector" />

                        <TextView
                            style="@style/menu_item_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/menu_cet" />

                    </LinearLayout>

                    <LinearLayout
                        android:visibility="invisible"
                        style="@style/menu_item"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/menu_emptyClassRoom"
                            style="@style/menu_item_icon"
                            android:src="@drawable/icon_emptyclassroom_selector" />

                        <TextView
                            style="@style/menu_item_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/menu_emptyClassRoom" />

                    </LinearLayout>



                    <LinearLayout
                        style="@style/menu_item"
                        android:visibility="invisible"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <ImageButton
                            android:id="@+id/menu_exam"
                            style="@style/menu_item_icon"
                            android:src="@drawable/icon_test_arrangement_selector" />

                        <TextView
                            style="@style/menu_item_label"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/menu_exam" />

                    </LinearLayout>

                </TableRow>

                <!--<TableRow-->
                    <!--android:layout_width="match_parent"-->
                    <!--android:layout_height="wrap_content"-->
                    <!--android:layout_marginTop="15dp"-->
                    <!--android:visibility="gone">-->

